package atividade;

public class Nodo {

	private int valor;
	private Nodo prox;

	public Nodo() {
		this.valor = 0;
		this.prox = null;
	}

	public Nodo(int valor, Nodo prox) {
		super();
		this.valor = valor;
		this.prox = prox;
	}

	public int getValor() {
		return valor;
	}

	public void setValor(int valor) {
		this.valor = valor;
	}

	public Nodo getProx() {
		return prox;
	}

	public void setProx(Nodo prox) {
		this.prox = prox;
	}
}
